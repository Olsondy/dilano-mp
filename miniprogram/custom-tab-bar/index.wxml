<view class="tab-bar">
  <view class="tab-bar-border"></view>
  <view 
    wx:for="{{list}}" 
    wx:key="index" 
    class="tab-bar-item {{selected === index ? 'item-active' : ''}}" 
    bindtap="switchTab" 
    data-path="{{item.pagePath}}" 
    data-index="{{index}}"
  >
    <view class="tab-icon-wrapper">
      <view class="icon-shape {{item.iconType}}"></view>
    </view>
    <text class="tab-text">{{item.text}}</text>
  </view>
</view>